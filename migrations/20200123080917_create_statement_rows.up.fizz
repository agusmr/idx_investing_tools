create_table("statement_rows") {
	t.Column("id", "uuid", {primary: true})
	t.Column("statement_id", "uuid", {})
	t.Column("row_title_id", "uuid", {})
	t.Column("row_order", "integer", {null: true})
	t.Column("row_description", "string", {null: true})
	t.Column("row_properties", "string", {null: true})
	t.Timestamps()
}